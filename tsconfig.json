{
  "compilerOptions": {
    "module": "ESNext",
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": "src",
    "target": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "@render": [
        "./render/index.ts"
      ],
      "types/*": [
        "types/*"
      ]
    },
  },
  "include": [
    "e2e/**/*.ts",
    "src/**/*"
  ],
}